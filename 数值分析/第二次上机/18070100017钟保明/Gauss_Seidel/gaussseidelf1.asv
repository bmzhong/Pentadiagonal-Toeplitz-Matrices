function [x,k]=gaussseidelf1(A,b,x0,ep,Nmax)
n=length(A);
k=0;
if nargin<5 
    Nmax=500;
end
if nargin<4
    ep=1e-5;
end
if nargin<3
    x0=zeros(n,1);
end
x=x0;
x0=x+2*ep;
while norm(x0-x,inf)>ep&&k<Nmax
    k=k+1;
    x0=x;
    y=x;
    for i=1:n
        z(i)=b(i);
        for j=1:n
            if j-=i
                z(i)=z(i)-A